@model LOWEN.Models.tblConfig
<div id="top">
    <div id="contentTop">
        <div id="location">
            <a title="" class="select">
                <i class="fa fa-globe" aria-hidden="true"></i>
                Lowen Việt Nam <i class="fa fa-angle-down" aria-hidden="true"></i>
            </a>
        </div>
        <div id="hotlineTop">
            <span><i class="fa fa-phone-square" aria-hidden="true"></i> Hỗ trợ khách hàng : <strong>@Model.HotlineIN </strong></span>
        </div>
        <div id="menuTop">
            <ul>
                <li><a href="/" title="Trang chủ"><i class="fa fa-home" aria-hidden="true"></i> Trang chủ</a></li>
                <li><a href="/tin-tuc" title="tin tức"><i class="fa fa-file-text-o" aria-hidden="true"></i> Tin tức</a></li>
                <li><a href="/tuyen-dung" title="tuyển dụng"><i class="fa fa-user" aria-hidden="true"></i> Tuyển dụng</a></li>
                <li><a href="/lien-he" title="Liên hệ"><i class="fa fa-question-circle" aria-hidden="true"></i> Liên hệ</a></li>
            </ul>
        </div>
    </div>
</div>
<div id="banner">
    <div id="contentBanner">
        <div id="menuLeft">
            <ul class="ul1">
                <li class="li1"><a href="#" title="Sản phẩm">Sản phẩm</a></li>
                <li class="li1"><a href="/dich-vu" title="Dịch vụ">Dịch vụ</a></li>
                <li class="li1"><a href="/Hệ thống phân phối" title="Hệ thống phân phối">Hệ thống phân phối</a></li>
            </ul>
        </div>
        <div id="logo">
            <a href="/" title="Lowen"><img src="@Model.Logo" alt="Lowen" /></a>
        </div>
        <div id="menuRight">
            <ul class="ul1">
                <li class="li1"><a href="" title="">Tải về</a></li>
            </ul>
            <div id="boxSearch">
                <div class="boxSearch">
                    <input type="search" name="txtSearch" id="txtSearch" class="txtSearch" placeholder="Search" />
                    <button type="submit" id="btnSubmit" class="btnSearch" name="btnSubmit">
                        <i class="fa fa-search" aria-hidden="true"></i>
                    </button>
                </div>
            </div>
            <div id="order">
                <a href="" class="orderDetail">
                    <i class="fa fa-cart-plus" aria-hidden="true"></i><span class="orderCount">0</span>
                </a>
            </div>
        </div>
    </div>
</div>
